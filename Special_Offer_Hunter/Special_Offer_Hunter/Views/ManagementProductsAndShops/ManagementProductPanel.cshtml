@model ManagementProductViewModel
@{
    ViewData["Title"] = "ManagementProductPanel";
}

<h1>ManagementProductPanel</h1>




        public string UserId { get; set; }

        public string Name { get; set; }
        public string Description { get; set; }

        public double Weight { get; set; }
        public double Height { get; set; }
        public string Picture { get; set; }

        public string Shop { get; set; }

        public string Code { get; set; }

        public double Price { get; set; }

        public string Category { get; set; }



<div class="container">
    <form>

        <div class="col col-lg-2">
            <input class="form-check-input" value="" id="SpecialOffer" asp-for="Name">
            <label class="form-check-label">
                Promocja na Produkt
            </label>
        </div>
        <div class="col col-lg-2">
            <input class="form-check-input" value="" id="SpecialOffer" asp-for="Weight">
            <label class="form-check-label">
                Promocja na Produkt
            </label>
        </div>
        <div class="col col-lg-2">
            <input class="form-check-input" value="" id="SpecialOffer" asp-for="Height">
            <label class="form-check-label">
                Promocja na Produkt
            </label>
        </div>
        <div class="col col-lg-2">
            <input class="form-check-input" value="" id="SpecialOffer" asp-for="Picture">
            <label class="form-check-label">
                Promocja na Produkt
            </label>
        </div>
        <div class="col col-lg-2">
            <input class="form-check-input" value="" id="SpecialOffer" asp-for="Shop">
            <select class="custom-select custom-select-sm" asp-for="Shop" asp-items="Html.GetEnumSelectList<SortType>()">
                @*<option id="sortMethod" selected="selected" value="">Please select</option>*@
            </select>
        </div>
        <div class="col col-lg-2">
            <input class="form-check-input" type="checkbox" value="" id="SpecialOffer" asp-for="Code">
            <label class="form-check-label">
                Promocja na Produkt
            </label>
        </div>
        <div class="col col-lg-2">
            <input class="form-check-input" value="" id="SpecialOffer" asp-for="Price">
            <label class="form-check-label">
                Promocja na Produkt
            </label>
        </div>
        <div class="col col-lg-2">
            <input class="form-check-input" value="" id="SpecialOffer" asp-for="Category">
            <select class="custom-select custom-select-sm" asp-for="Category" asp-items="Html.GetEnumSelectList<SortType>()">
                @*<option id="sortMethod" selected="selected" value="">Please select</option>*@
            </select>
        </div>




    </form>
</div>